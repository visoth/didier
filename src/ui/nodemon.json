{
  "restartable": "rs",
  "ignore": [
    "*.json",
    ".git",
    "node_modules/**/node_modules",
    "dist",
    "coverage",
    "tests",
    "src"
  ],
  "verbose": false,
  "execMap": {
    "js": "babel-node"
  },
  "events": {
    "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "watch": [
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js"
}
